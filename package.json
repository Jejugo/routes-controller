{
  "name": "meli_proxy",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git:git@github.com:Jejugo/meli_proxy.git"
  },
  "scripts": {
    "dev": "npm-run-all -p dev:*",
    "dev:server": "cp config/config.dev.json config/index.json; nodemon server/index.js",
    "dev:queue": "cp config/config.dev.json config/index.json; nodemon server/queue.js",
    "dev:debug": "cp config/config.dev.json config/index.json; node --inspect server/index.js",
    "sequelize:migrate": "npx sequelize-cli db:migrate --env sequelize",
    "test": "mocha 'test/**/*.js' --recursive --timeout 60000 --exit",
    "docker:run": "docker run -it -p 3002:3002  meli_proxy:latest",
    "docker:db:redis": "docker run --name redis_proxy -p 6379:6379 -d redis",
    "docker:db:run-mysql": "docker run --rm --name=my-mysql --env MYSQL_ROOT_PASSWORD=password --detach --publish 3306:3306 mysql",
    "sequelize:generate:migration": "npx sequelize migration:generate --name [name_of_your_migration]"
  },
  "author": "Jeff Goes",
  "license": "ISC",
  "dependencies": {
    "bluebird": "^3.7.2",
    "body-parser": "^1.19.0",
    "bull": "^3.20.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "chai-http": "^4.3.0",
    "cookie-parser": "^1.4.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "mercadolibre-nodejs-sdk": "^3.0.1",
    "moment": "^2.29.1",
    "mysql2": "^2.1.0",
    "node-fetch": "^2.6.1",
    "redis": "^3.0.2",
    "request": "^2.88.2",
    "sequelize": "^6.3.3",
    "sinon": "^9.2.4",
    "sinon-chai": "^3.5.0",
    "uuid": "^8.3.0"
  },
  "devDependencies": {
    "express-rate-limit": "^5.2.3",
    "nodemon": "^2.0.7",
    "rootpath": "^0.1.2",
    "sequelize-cli": "^6.2.0"
  }
}
